<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.todoApp.dao.UserDao">	
	<insert id="insertUser">
		insert into `user` set 
							`uid`=#{uid}, 
							`pass`=#{pass}, 
							`grade`=0;
	</insert>
</mapper>